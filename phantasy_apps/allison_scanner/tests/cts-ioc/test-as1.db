# ISRC1
record(calc, "FE_SCS1:EMS_D0739:VBS:isOn") {
  field(CALC, A)
  field(INPA, "FE_SCS1:EMS_D0739:VBS:setOn CPP MS")
}
record(ai, "FE_SCS1:EMS_D0739:VBS:setOn") {
  field(PINI, "YES")
  field(VAL, 1)
}
record(calc, "FE_SCS1:EMS_D0739:VBS:VoltageMeasure") {
  field(CALC, A)
  field(EGU, "V")
  field(INPA, "FE_SCS1:EMS_D0739:VBS:VoltageSet CPP MS")
}
record(ai, "FE_SCS1:EMS_D0739:VBS:VoltageSet") {
  field(PINI, "YES")
  field(EGU, "V")
  field(VAL, -999)
}

