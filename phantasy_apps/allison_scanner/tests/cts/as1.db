# SCS1 EMS
# Bias Voltage On/Off
record(calc, "FE_SCS1:EMS_D0739:VBS:isOn") {
  field(CALC, A)
  field(INPA, "FE_SCS1:EMS_D0739:VBS:setOn CPP MS")
}
record(ai, "FE_SCS1:EMS_D0739:VBS:setOn") {
  field(PINI, "YES")
  field(VAL, 1)
}
# Bias Voltage readback
record(calc, "FE_SCS1:EMS_D0739:VBS:VoltageMeasure") {
  field(CALC, A)
  field(EGU, "V")
  field(INPA, "FE_SCS1:EMS_D0739:VBS:VoltageSet CPP MS")
}
# Bias Voltage setpoint
record(ai, "FE_SCS1:EMS_D0739:VBS:VoltageSet") {
  field(PINI, "YES")
  field(EGU, "V")
  field(VAL, -999)
}

# (X) pos OUT limit
record(calcout, "_FE_SCS1:EMS_D0739:LMPOS_RSTS_DRV1") {
    field(INPA, "FE_SCS1:EMS_D0739:DRV1.RBV CPP")
    field(DOPT, "Use CALC")
    field(CALC, "A>=150")
    field(OOPT, "Every Time")
    field(OUT, "FE_SCS1:EMS_D0739:LMPOS_RSTS_DRV1 PP")
}

# (X) pos IN limit
record(calcout, "_FE_SCS1:EMS_D0739:LMNEG_RSTS_DRV1") {
    field(INPA, "FE_SCS1:EMS_D0739:DRV1.RBV CPP")
    field(DOPT, "Use CALC")
    field(CALC, "A<50")
    field(OOPT, "Every Time")
    field(OUT, "FE_SCS1:EMS_D0739:LMNEG_RSTS_DRV1 PP")
}

# (Y) pos OUT limit
record(calcout, "_FE_SCS1:EMS_D0739:LMPOS_RSTS_DRV2") {
    field(INPA, "FE_SCS1:EMS_D0739:DRV2.RBV CPP")
    field(DOPT, "Use CALC")
    field(CALC, "A>=150")
    field(OOPT, "Every Time")
    field(OUT, "FE_SCS1:EMS_D0739:LMPOS_RSTS_DRV2 PP")
}

# (Y) pos IN limit
record(calcout, "_FE_SCS1:EMS_D0739:LMNEG_RSTS_DRV2") {
    field(INPA, "FE_SCS1:EMS_D0739:DRV2.RBV CPP")
    field(DOPT, "Use CALC")
    field(CALC, "A<50")
    field(OOPT, "Every Time")
    field(OUT, "FE_SCS1:EMS_D0739:LMNEG_RSTS_DRV2 PP")
}

# (X) Enable/un-Enable
record(calcout, "_FE_SCS1:EMS_D0739:ENNEG_CMD_DRV1") {
    field(INPA, "FE_SCS1:EMS_D0739:ENNEG_CMD_DRV1 CPP")
    field(DOPT, "Use CALC")
    field(CALC, "A")
    field(OOPT, "Every Time")
    field(OUT, "FE_SCS1:EMS_D0739:ENNEG_RCMD_DRV1 PP")
}
# (Y) Enable/un-Enable
record(calcout, "_FE_SCS1:EMS_D0739:ENNEG_CMD_DRV2") {
    field(INPA, "FE_SCS1:EMS_D0739:ENNEG_CMD_DRV2 CPP")
    field(DOPT, "Use CALC")
    field(CALC, "A")
    field(OOPT, "Every Time")
    field(OUT, "FE_SCS1:EMS_D0739:ENNEG_RCMD_DRV2 PP")
}

# (X) Interlock: if IN changing from 0 to 1, latch interlock to 1
record(calcout, "_FE_SCS1:EMS_D0739:LMPOS_LTCH_DRV1") {
    field(INPB, "FE_SCS1:EMS_D0739:LMNEG_RSTS_DRV1 CPP")
    field(DOPT, "Use OCAL")
    field(CALC, "B")
    field(OCAL, "1")
    field(OOPT, "Transition To Non-zero")
    field(OUT, "FE_SCS1:EMS_D0739:LMPOS_LTCH_DRV1 PP")
}
# (Y) Interlock:  if IN changing from 0 to 1, latch interlock to 1
record(calcout, "_FE_SCS1:EMS_D0739:LMPOS_LTCH_DRV2") {
    field(INPB, "FE_SCS1:EMS_D0739:LMNEG_RSTS_DRV2 CPP")
    field(DOPT, "Use CALC")
    field(CALC, "B")
    field(OCAL, "1")
    field(OOPT, "Transition To Non-zero")
    field(OUT, "FE_SCS1:EMS_D0739:LMPOS_LTCH_DRV2 PP")
}

# Reset Interlock: Set intelock to 0 if OUT for both X and Y
record(calcout, "_FE_SCS1:DIAG_D0739:RST_CMD") {
    field(INPA, "FE_SCS1:DIAG_D0739:RST_CMD CPP")
    field(DOPT, "Use CALC")
    field(CALC, "A")
    field(OOPT, "Transition To Non-zero")
    field(OUT, "_FE_SCS1:EMS_D0739:RST_ITLK PP")
}
record(fanout, "_FE_SCS1:EMS_D0739:RST_ITLK") {
    field(LNK0, "_FE_SCS1:EMS_D0739:RST_ITLK_X")
    field(LNK1, "_FE_SCS1:EMS_D0739:RST_ITLK_Y")
    field(LNK2, "_FE_SCS1:DIAG_D0739:RST_CMD_0")
}
# reset RST_CMD to 0 after fanout
record(calcout, "_FE_SCS1:DIAG_D0739:RST_CMD_0") {
    field(INPA, "FE_SCS1:DIAG_D0739:RST_CMD NPP")
    field(CALC, "A")
    field(OCAL, "0")
    field(DOPT, "Use OCAL")
    field(OOPT, "When Non-zero")
    field(OUT, "FE_SCS1:DIAG_D0739:RST_CMD PP")
}
# when OUT, set interlock to OK (0)
record(calcout, "_FE_SCS1:EMS_D0739:RST_ITLK_X") {
    field(INPA, "FE_SCS1:EMS_D0739:LMPOS_RSTS_DRV1 NPP")
    field(CALC, "A")
    field(DOPT, "Use OCAL")
    field(OCAL, "0")
    field(OOPT, "When Non-zero")
    field(OUT, "FE_SCS1:EMS_D0739:LMPOS_LTCH_DRV1 PP")
}
record(calcout, "_FE_SCS1:EMS_D0739:RST_ITLK_Y") {
    field(INPA, "FE_SCS1:EMS_D0739:LMPOS_RSTS_DRV2 NPP")
    field(CALC, "A")
    field(DOPT, "Use OCAL")
    field(OCAL, "0")
    field(OOPT, "When Non-zero")
    field(OUT, "FE_SCS1:EMS_D0739:LMPOS_LTCH_DRV2 PP")
}
