    field(OUT, "FE_SCS1:EMS_D0739:LMPOS_RSTS_DRV2 PP")
}

# (Y) pos IN limit
record(calcout, "_FE_SCS1:EMS_D0739:LMNEG_RSTS_DRV2") {
    field(INPA, "FE_SCS1:EMS_D0739:DRV2.RBV CPP")
    field(DOPT, "Use CALC")
    field(CALC, "A<50")
